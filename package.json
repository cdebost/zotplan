{
  "name": "zotplan",
  "version": "0.0.0",
  "description": "An application for making multi-year plans for UCI programs, with checks for prerequisites, major requirements, course availability, etc.",
  "main": "dist",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test scripts/test.sh",
    "crawler": "node dist/crawler"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cdebost/zotplan.git"
  },
  "author": "Corentin Debost",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cdebost/zotplan/issues"
  },
  "homepage": "https://github.com/cdebost/zotplan#readme",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "google-auth-library": "^0.10.0",
    "if-env": "^1.0.0",
    "mongoose": "^4.8.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "request": "^2.79.0",
    "supertest": "^2.0.1",
    "url-parse": "^1.1.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
